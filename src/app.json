{
	"expo": {
		"name": "PetSprout",
		"slug": "petsprout",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./frontend/resources/images/Logo/LogoGreenNonTransparent.jpeg",
		"splash": {
			"image": "./frontend/resources/images/Logo/LogoGreenNonTransparent.jpeg",
			"resizeMode": "contain",
			"backgroundColor": "#bde4a8"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"android": {
			"googleServicesFile": "./google-services.json",
			"softwareKeyboardLayoutMode": "pan",
			"package": "com.phylimchordata.PetSprout",
			"versionCode": "2.0.0",
			"adaptiveIcon": {
				"foregroundImage": "./frontend/resources/images/Logo/LogoGreenNonTransparent.jpeg",
				"backgroundColor": "#FFFFFF"
			}
		},
		"ios": {
			"bundleIdentifier": "com.phylimchordata.PetSprout",
			"buildNumber": "2.0.0"
		},
		"web": {
			"favicon": "./frontend/resources/images/Logo/LogoGreenNonTransparent.jpeg"
		},
		"plugins": [
			[
			  "expo-notifications",
			  {
				"icon": "./frontend/resources/images/Icon.jpeg",
				"color": "#ffffff"
			  }
			]
		  ]
	}
}
