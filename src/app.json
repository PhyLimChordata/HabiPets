{
  "expo": {
    "name": "HabiPets",
    "slug": "HabiPets",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./frontend/resources/images/Logo.png",
    "scheme": "habipets",
    "splash": {
      "image": "./frontend/resources/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./frontend/resources/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.habipets",
      "versionCode": 1
    },
    "web": {
      "favicon": "./frontend/resources/assets/favicon.png"
    }
  }
}
